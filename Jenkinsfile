node {

    stage('Checkout'){
        echo "Checking out code... !"
    }

    stage('Build'){
        echo "Doing Build !"
    }

    stage('Test'){
        echo "Doing Testing !"
    }

    stage('Artifacts'){
        echo "Where do I put artifact ? "
    }

    stage('Deploy'){
        echo "Deploying your code, don't know where ! "
    }

    stage('TaggingAndNotifications'){
        echo "I am closing the shop, last thing before I finish ! "
    }

}